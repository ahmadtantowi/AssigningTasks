@model List<AssigningTasks.Sample.Data.Target>

@{
    Layout = "_BaseLayout";
    ViewData["Title"] = "Pengguna";
}

<h1>@ViewData["Title"]</h1>

<p>
    @*<a asp-action="Create">Create New</a>*@
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model[0].TargetId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Latitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Longitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].LastRequest)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TargetId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Latitude)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Longitude)
                </td>
                <td>
                    @Html.DisplayFor(modelItem=>item.LastRequest)
                </td>
                @*<td>
                    <a asp-action="Edit" asp-route-id="@item.TargetId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.TargetId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.TargetId">Delete</a>
                </td>*@
            </tr>
        }
    </tbody>
</table>