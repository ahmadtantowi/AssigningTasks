@model List<AssigningTasks.Sample.ViewModels.CandidateViewModel>

<table class="table">
    <thead>
        <tr>
            <th hidden>@Html.DisplayNameFor(model => model[0].Id)</th>
            <th>@Html.DisplayNameFor(model => model[0].Name)</th>
            <th>@Html.DisplayNameFor(model => model[0].DistanceToTarget)</th>
            <th>@Html.DisplayNameFor(model => model[0].Load)</th>
            <th hidden>@Html.DisplayNameFor(model => model[0].Latitude)</th>
            <th hidden>@Html.DisplayNameFor(model => model[0].Longitude)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td hidden>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.DistanceToTarget</td>
                <td>@employee.Load</td>
                <td hidden>@employee.Latitude</td>
                <td hidden>@employee.Longitude</td>
            </tr>
        }
    </tbody>
</table>
